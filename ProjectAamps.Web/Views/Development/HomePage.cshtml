@model IEnumerable<AAMPS.Clients.AampService.SelectRelevantDevelopmentResult>
@{
    ViewBag.Title = "HomePage";
}

<div class="jumbotron" style="background-color:#eee; margin-top:0px;padding-top:5px;padding-bottom:5px;margin-top: 20px;">
    <div class="row text-center">
        <h3>PLEASE SELECT YOUR DEVELOPMENT</h3>
    </div>
</div>
<div class="row">
    @foreach (var development in Model)
    {
        <div class="col-md-3" style="margin-bottom:15px">
            <a style="color:#eee;text-decoration:none" href='@Url.Action("Dashboard", "Development", new { development.DevelopmentID})'>
                <div class="thumbnail">
                    <div class="caption">
                        <h4><span class="text-content"><span>@development.DevelopmentDescription</span></span></h4>
                    </div>
                    <img id="" class="img-thumbnail" src='@Url.Content("~/Images/" + development.DevelopmentUrlImage)' style="width:100%;height:150px;" />
                </div>
            </a>
        </div>
    }
</div>
<br /><br />
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script>

    $(function () {
        @*$("#developmentLogo").removeClass('hide');
        $("#developmentLogo").attr("src", '@Url.Content("~/Images/" + Session["DevelopmentImage"].ToString())');*@
        $('.thumbnail').hover(
       function () {

           $(this).find('.caption').fadeIn(150); //.fadeIn(250)
       },
       function () {
           $(this).find('.caption').fadeOut(150); //.fadeOut(205)
       }
   );
    })
</script>

 