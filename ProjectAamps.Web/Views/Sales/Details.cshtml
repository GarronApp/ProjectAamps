@{
    ViewBag.Title = "Sales Details";
    Layout = "_LayoutSales.cshtml";
}

@*<link href="~/Content/bootstrap-datepicker.css" rel="stylesheet" />*@
<style>
    .datepicker {
        z-index: 1151 !important;
    }

    .datepicker {
        z-index: 10000;
    }

    .panel-title > a:before {
        font-family: FontAwesome;
        content: "\f056";
        padding-right: 5px;
        float: right;
        text-decoration: none;
        font-size: 15px;
    }

    .panel-title > a.collapsed:before {
        content: "\f055";
    }
</style>

<div class="panel panel-default">
    <a data-toggle="collapse" data-parent="#accordion"  href="#collapseOne" style="text-decoration:none">
        <div class=" panel-heading" id="divPanelReserved">
            <h4 class="panel-title">
                Reservation

                <span class="pull-right"><i id="showPanelReservedOpenClose" class="fa fa-plus-circle" style="font-size:18px"></i></span>
                <input type="button" class="close pull-right btn-cancel" value="RETURN" data-dismiss="modal" onclick="window.location.href='@Url.Action("Dashboard","Development")'" />
            </h4>
        </div>
    </a>
    <div id="collapseOne" class="panel-collapse collapse">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6">
                    <form class="form-horizontal" id="SaleReservedForm">
                        <input type="text" class="form-control" id="CurrentIndividualID" name="CurrentIndividualID" value="" style="display:none">
                      
                        <div class="form-group">
                            <label for="txtStatus" class="col-sm-4 control-label">Status</label>
                            <div class="col-sm-8">
                                <input type="text" value="" readonly="readonly" class="form-control" id="txtSaleReservedPanelStatus" placeholder="status">
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="txtReservationDate" class="col-sm-4 control-label">Reservation Date</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control datepicker" id="txtReservationDate" name="SaleReservationDt" data-date-format="dd/mm/yyyy" placeholder="">
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="txtExpiryDate" class="col-sm-4 control-label">Expiry Date</label>
                            <div class="col-sm-8">
                                <input type="text" readonly="readonly"  class="form-control datepicker" id="txtExpiryDate" name="SaleReservationExpiryDt" data-date-format="dd/mm/yyyy" placeholder="">
                            </div>
                        </div>
                        <div id="divReservationExtend">
                            <div class="form-group">
                                <label for="selectTimeExtention" class="col-sm-4 control-label">Reservation Time Extention</label>
                                <div class="col-sm-1">
                                    <div class="checkbox">
                                        <label><input type="checkbox" id="checkReservationTimeExtention" value=""></label>
                                    </div>
                                </div>
                                <div class="col-sm-7">
                                    <select class="form-control" id="selectTimeExtention" disabled="disabled">
                                        <option selected="selected"></option>
                                        <option value="1">1 Day</option>
                                        <option value="2">2 Days</option>
                                        <option value="3">3 Days</option>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="txtExtentionDate" class="col-sm-4 control-label">Extention Date</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control datepicker" id="txtExtentionDate" name="SaleReservationExtentionDt" data-date-format="dd/mm/yyyy" placeholder="">
                                </div>
                            </div>
                        </div>

                        <div class=" form-group">
                            <div class="col-sm-offset-4 col-sm-8">
                                <input type="button" id="btnUpdateReservation" class="btn btn-primary btn-sm" value="Update Reservation" />
                            </div>
                        </div>

                    </form>
                </div>
                <div class="col-md-6">
                    <form class="form-horizontal" id="individualFormReserved">
                        <div class="form-group">
                            <input type="text" class="form-control" id="IndividualID" name="IndividualID" value="" style="display:none">
                            <input type="text" id="IsNewIndividual" name="IsNewIndividual" value="1" class="hide" />
                            <label for="txtFirstName" class="col-sm-2 control-label">First Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtIndividualName" name="IndividualName" placeholder="first name">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtLastName" class="col-sm-2 control-label">Last Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtIndividualSurname" name="IndividualSurname" placeholder="last name">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtCellNumber" class="col-sm-2 control-label">Cell No</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control cellphone" id="txtIndividualContactCell" name="IndividualContactCell" placeholder="cell number">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtWorkNumber" class="col-sm-2 control-label">Work No</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control workphone" id="txtIndividualContactWork" name="IndividualContactWork" placeholder="work number">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtEmail" class="col-sm-2 control-label">Email Address</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtIndividualEmail" name="IndividualEmail" placeholder="email address">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="selectPreferdContactMethod" class="col-sm-2 control-label">Contact Method</label>
                            <div class="col-sm-10">
                                <select class="form-control" id="PreferedContactMethodID" name="PreferedContactMethodID"></select>
                            </div>
                        </div>


                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <input type="button" id="btnReservedUpdatePersonDetails" class="btn btn-primary btn-sm" value="Update Individual's Details" />
                            </div>
                        </div>

                    </form>

                </div>
            </div>
        </div>
    </div>
</div>

<div class="panel panel-default" id="pendingPanel">
