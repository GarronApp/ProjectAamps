                                <label for="checkSalesDepositProofBt" class="col-sm-4 control-label">Deposit Proof Date</label>
                                <div class="col-sm-1">
                                    <div class="checkbox">
                                        <label><input type="checkbox" id="checkSalesDepositProofBt" @*name="SalesDepositProofBt"*@ value=""></label>
                                    </div>
                                </div>
                                <div class="col-sm-7">
                                    <input type="text" class="form-control datepicker" disabled="disabled" id="txtSalesDepositProofDt" name="SalesDepositProofDt" data-date-format="dd/mm/yyyy" placeholder="">
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="txtDepositPaid" class="col-sm-4 control-label">Upload Proof</label>
                                <div class="col-sm-8">
                                    @*<span class="btn btn-default btn-file">
                                            Browse <input type="file">
                                        </span>*@
                                    <a class='btn btn-primary' href='javascript:;'>
                                        Browse...
                                        <input type="file" style='position:absolute;z-index:2;top:0;left:0;filter: alpha(opacity=0);-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";opacity:0;background-color:transparent;color:transparent;' name="file_source" size="40" onchange='$("#upload-file-info").html($(this).val());'>
                                    </a>
                                    &nbsp;
                                    <span class='label label-info' id="upload-file-info"></span>
                                </div>
                            </div>
                        </div>


                        <div class="form-group">
                            <div class="col-sm-offset-4 col-sm-8">
                                <input type="button" class="btn btn-primary btn-sm" id="btnUpdateReservedSale" value="Update Sale Details" />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="col-md-6">
                    <form class="form-horizontal" id="individualFormPending">
                        <input type="text" class="form-control" id="IndividualUpdateID" name="IndividualID" value="" style="display:none">
                        <input type="text" id="IsNewIndividual" name="IsNewIndividual" value="0" class="hide" />
                        <div class="form-group">
                            <label for="txtFirstName" class="col-sm-2 control-label">First Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtFirstNamePending" disabled="disabled" name="IndividualName" placeholder="first name">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtLastName" class="col-sm-2 control-label">Last Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtLastNamePending" disabled="disabled" name="IndividualSurname" placeholder="last name">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtCellNumber" class="col-sm-2 control-label">Cell No</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control cellphone" id="txtCellNumberPending" disabled="disabled" name="IndividualContactCell" placeholder="cell number">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtWorkNumber" class="col-sm-2 control-label">Work No</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control workphone" id="txtWorkNumberPending" disabled="disabled" name="IndividualContactWork" placeholder="work number">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtEmail" class="col-sm-2 control-label">Email Address</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtEmailPending" disabled="disabled" name="IndividualEmail" placeholder="email address">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="selectPreferdContactMethod" class="col-sm-2 control-label">Contact Method</label>
                            <div class="col-sm-10">
                                <select class="form-control" id="txtPendingPreferedContactMethod" disabled="disabled" name="PreferedContactMethodID"></select>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <input type="button" id="btnPendingUpdatePersonDetails" class="btn btn-primary btn-sm" value="Update Contact Details" />
                            </div>
                        </div>

                    </form>

                </div>
            </div>
        </div>
    </div>
</div>

